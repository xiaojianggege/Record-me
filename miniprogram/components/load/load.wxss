.remind-box {
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translate(-50%,-50%);
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 100;
}
.remind-img {
  width: 250rpx;
  height: 250rpx;
  padding-bottom: 25rpx;
  z-index: 100;
}